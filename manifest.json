{
    "manifest_version": 2,
    "name": "Mild Content",
    "short_name": "MCon",
    "version": "1.0.0",
    "description": "Replace punctuation marks with emoji",
    "icons": {
        "16":"icon16.png",
        "48":"icon48.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_title": "Mild Content"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["main.js"]
    }]
    
}